<div class="mb-3 text-center">
    <%if(success && success.length){%>
      
      <div class="alert alert-success" role="alert">
        <%= success %> 
      </div>


    <%}%>
  </div>
  <div class="mb-3 text-center">
     <%if(error && error.length){%>

      <div class="alert alert-danger" role="alert">
        <%= error %> 
      </div>

    <%}%>
</div>